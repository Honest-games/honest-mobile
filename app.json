{
  "expo": {
    "name": "Честно",
    "slug": "haur",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/appstore.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#f3f3f3"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon": "./assets/images/appstore.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.tige21.haur",
      "buildNumber": "7"
    },
    "android": {
      "icon": "./assets/images/playstore.png",
      "package": "com.tige21.haur"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "32b16ea2-3dc9-45d9-b424-41453f69c5a6"
      }
    },
    "owner": "tinygeekslab"
  }
}
